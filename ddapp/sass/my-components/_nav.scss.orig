.sidebar {
    position: fixed;
    top: 0;
    left: 0;
    max-height: 100vh;
    height: 100vh;
    width: 64px;
    padding: 40px 0;
    background-color: $brand-primary;
    transition: width 0.3s;
    &__fake {
        height: 100%;
        width: 90px;
        float: left;
    }
    &__list {
        padding: 0;
        margin-bottom: 20px;
        list-style-type: none;
    }
    &__link {
        position: relative;
        z-index: 2;
        color: #fff;
        display: block;
        padding: 10px 17px;
        transition: background-color 0.3s, color 0.3s;
        font-size: 0;
        .icon {
            position: relative;
            color: inherit;
            fill: #fff;
            width: 30px;
            height: 50px;
        }
    }
    &__item {
        position: relative;

        line-height: 60px;
        z-index: 1;
        white-space: nowrap;
        background-color: $brand-primary;
        &:focus {
            outline: none;
            box-shadow: none;
        }
        &:hover,
        &.active {
            .sidebar__link{
                background-color: #58BFE8;
            }
        }
    }
    &__name {
        position: relative;
        display: none;
        vertical-align: middle;
        overflow: hidden;
        font-size: 22px;
        font-weight: 300;
        opacity: 0;
        visibility: hidden;
        transition: opacity 0.3s, visibility 0.3s, max-width 0.3s, max-height 0.3s;
        z-index: 1;
        line-height: 40px;
        white-space: normal;
    }
    &.active {
        width: 250px!important;
        text-align: left;
        .jspContainer,
        .jspPane {
            width: 250px !important;
        }
        /* .sidebar__item {

            &:after {
                border-width: 45px 0 45px 30px;
            }
        } */
        .sidebar__name {
            display: inline-block;
            opacity: 1;
            visibility: visible;
            max-width: 300px;
            margin-left: 40px;
        }
        .menu__arrow {
            transform: rotate(180deg);
        }
        .menu__arrow_holder {
            text-align: right;
            padding-right: 30px;
        }
    }
    z-index: 10;
}

.menu__arrow {
    display: inline-block;
    margin-bottom: 25px;
    cursor: pointer;
    transition: transform 0.3s;
    .icon {
        fill: #fff;
    }
    &_holder {
        text-align: center;
    }
}


/**
 * Tabs
 */

 .nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover {
    border-bottom: 1px solid #fff;
 }

.nav-tabs {
    border-bottom: 1px solid #9f9f9f;
    >li {
        &:first-child {
            margin-left: 30px;
        }
        &.active {

            a {
                border-top: 2px solid $brand-primary;
                &:focus {
                    border-top: 2px solid $brand-primary;
                }
            }
            &:hover {
                a {
                    border-top: 2px solid $brand-primary;
                }
            }
                    }
        >a {
            font-size: 22px;
            font-weight: 300;
            color: $text-color;
            padding: 2px 30px;
        }
    }
}

.tab-content {
    padding-top: 30px;
    padding-left: 30px;
    .tab-content {
        padding: 0;
    }
}


/**
 * Tools pagination
 */


.pagination {
    &__list,
    &__arrow,
    &__info,
    &__item {
        display: inline-block;
        vertical-align: middle;
        font-size: 24px;
    }
    &__item {
        cursor: pointer;
        &.active {
            cursor: default;
            border: 1px solid $brand-primary;
        }
        span {
            display: block;
            padding: 5px 20px;
            color: $second-color;
        }
    }
    &__info {
        margin-right: 60px;
        color: $gray-light;
        font-size: 16px;
        @media (max-width: 1400px) {
            margin-right: 20px;
        }
    }
    &__link, &__arrow {
        padding: 5px 20px;
        display: block;
        color: $second-color;
    }
    &__link {
        font-size: 20px;
    }

    &__list {
        padding: 0;
        margin: 0;
        list-style-type: none;
    }

}

